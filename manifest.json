{
    "name": "NemKommentál",
    "version": "0.2",
    "manifest_version": 2,
    "description": "Nem a kommentekért jöttél.",
    "icons": { "16": "16.png",
           "48": "48.png",
          "128": "128.png"
    },
    "content_scripts": [ {
        "js": [ "jquery.min.js", "content_script.js"  ],
        "matches": [ "http://*/*", "https://*/*" ]
    } ],

    "permissions" : [
        "tabs",
        "http://*/*"
            ]
}